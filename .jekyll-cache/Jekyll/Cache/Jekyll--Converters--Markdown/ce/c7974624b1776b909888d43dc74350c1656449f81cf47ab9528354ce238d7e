    function resizeIframe(obj) {
        obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
    }
</script>

<div>
    <iframe width="800" frameborder="0" src="/assets/20200428_map.html" onload="resizeIframe(this)"></iframe>
</div>

